{
  "name": "steedos",
  "version": "1.20.14",
  "private": true,
  "scripts": {
    "start": "env-cmd -f ./.env.local meteor run --settings settings.json --port 3100",
    "osassets": "cp -r node_modules/@salesforce-ux/design-system/assets/* public/assets",
    "build": "yarn && yarn os && yarn clear && env-cmd -f ./.env.local rm -rf .dist/bundle && meteor build --directory .dist --server-only && node .scripts/build-after.js",
    "testbuild": "cp -rf .dist/* ../steedos-contracts-app/node_modules/steedos-server/",
    "login": "npm login --registry https://registry.npmjs.org",
    "pub": "cd .dist && npm publish --registry https://registry.npmjs.org",
    "postinstall": "node .scripts/npm-postinstall.js",
    "prepare": "node .scripts/npm-postinstall.js",
    "clear": "rm -rf node_modules/@steedos/auth/node_modules/@steedos node_modules/@steedos/core/node_modules/@steedos node_modules/@steedos/objectql/node_modules/@steedos node_modules/@steedos/plugin-jsreport/node_modules/@steedos node_modules/@steedos/accounts/node_modules/@steedos",
    "osy": "cd ../object-server/ && yarn",
    "os": "cp -r ../object-server/packages/* node_modules/@steedos/ && cp -r ../object-server/apps/contracts/src/* node_modules/@steedos/app-contracts/src/",
    "oss": "cp -r ../object-server/packages/standard-objects/* node_modules/@steedos/standard-objects/ && yarn start",
    "osw": "cd ../steedos-webapp/ && yarn build-lib && cd ../creator/ && cp -r ../steedos-webapp/lib/* node_modules/@steedos/react/lib/",
    "sr": "cp -r ../steedos-plugin-stimulsoft-report/lib/* node_modules/@steedos/plugin-stimulsoft-report/lib/ && rm -rf node_modules/@steedos/plugin-stimulsoft-report/node_modules/@steedos"
  },
